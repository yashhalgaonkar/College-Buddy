        </activity>
        <activity android:name=".AttActivity" />
        <activity android:name=".SubActivity" />
        <activity android:name=".ExtActivity" />
        <activity android:name=".ttActivity" />
        <activity android:name=".vitActivity" />
        <activity android:name=".offActivity"></activity>
    </application>

</manifest>